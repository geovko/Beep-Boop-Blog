<!-- Loop over each object in the projects array -->
<h1>B.B.Blog</h1>
<small>A blog for all the beep boop ideas</small>
{{#each posts}}
  <div class="row mb-4 post">
    <div class="col-md-5">
      <h2><a href="/posts/{{id}}">{{title}}</a></h2>
      <p>{{content}}</p>
      <p>{{user.name}}
        -
        {{dateCreated}}</p>
    </div>
  </div>
{{/each}}